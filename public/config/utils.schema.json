{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://join.iwgb.org.uk/config/schema/utils.schema.json",
    "title": "Jobs",
    "description": "IWGB join process utility schemas",
    "definitions": {
        "id": {
            "type": "string",
            "pattern": "^[a-z\\-]+$"
        },
        "airtable-id": {
            "type": "string",
            "pattern": "^rec[a-zA-Z0-9]{14}$"
        },
        "typeform-form-id": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9]{6}$"
        }
    }
}